{
  "name": "react-client-starter",
  "version": "1.0.0",
  "description": "Get started with React, Redux, and React-Router!",
  "main": "index.js",
  "engines": {
    "node": ">=4.2.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "babel-node bin/compile",
    "lint": "eslint . ./",
    "lint:fix": "npm run lint -- --fix",
    "start": "npm run startint -- --no_debug",
    "start:quiet": "npm run startint -- --no_debug --no_log",
    "start:debug": "npm run startint",
    "startint": "better-npm-run start",
    "dev": "better-npm-run dev",
    "dev:nw": "npm run dev -- --nw",
    "dev:no-debug": "npm run dev -- --no_debug",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "deployprod": "better-npm-run deployprod",
    "flow:check": "babel-node bin/flow-check"
  },
  "betterScripts": {
    "dev": {
      "command": "nodemon --exec babel-node bin/server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "NODE_ENV": "production",
        "CHECK": "true",
        "DEBUG": "app:*"
      }
    },
    "deployprod": {
      "command": "npm run clean && npm run compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "babel-node bin/server",
      "env": {
        "DEBUG": "app:*",
        "NOLINT": "true"
      }
    },
    "test": {
      "command": "babel-node ./node_modules/karma/bin/karma start bin/karma",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PalladiumConsulting/react-client-starter.git"
  },
  "author": "David Zukowski <david@zuko.me> (http://zuko.me)",
  "license": "MIT",
  "dependencies": {
    "better-npm-run": "0.0.8",
    "classnames": "^2.2.4",
    "co-request": "^1.0.0",
    "debug": "^2.2.0",
    "history": "^2.1.1",
    "iconv-lite": "^0.4.13",
    "invariant": "^2.2.1",
    "koa": "^2.0.0-alpha.3",
    "koa-connect-history-api-fallback": "^0.3.0",
    "koa-convert": "^1.2.0",
    "koa-static": "^2.0.0",
    "lodash": "^4.11.1",
    "material-ui": "^0.15.0-beta.2",
    "moment": "^2.13.0",
    "react": "^15.0.1",
    "react-document-title": "^2.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.3.0",
    "react-router-redux": "^4.0.4",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-promise-middleware": "^3.0.0",
    "redux-thunk": "^2.0.1",
    "rx.disposables": "^1.0.0",
    "url": "^0.11.0",
    "whatwg-fetch": "^0.11.0",
    "yargs": "^3.18.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "babel-runtime": "^6.6.1",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.3.0",
    "chai-enzyme": "^0.4.2",
    "cheerio": "^0.20.0",
